{
  "resourceType": "PlanDefinition",
  "id": "IMMZ.DT.05.Hib",
  "url": "http://fhir.org/guides/who/Immz/PlanDefinition/IMMZ.DT.05.Hib",
  "name": "IMMZ.DT.05.Hib",
  "description": "if child or person has not been vaccinated, give Hib vaccine between 4 and 18 months",
  "action": [ {
    "title": "Immunize Patient for Hib - No Doses",
    "description": "Provide Hib immunizations – using the \"Hib Vaccine immunization – NO PREVIOUS\" schedule (1 dose scheme)",
    "condition": [ {
      "kind": "applicability",
      "expression": {
        "description": "\"Hib vaccine immunization history\" = \"No-doses and > 12 months old and no known allergies to any component of the vaccine\"",
        "language": "text/cql",
        "expression": "Should vaccinate patient for Hib because no doses"
      }
    } ],
    "definitionCanonical": "Https://placeholder.org/fhir/ActivityDefinition/Immunize%20Patient%20for%20Hib%20-%20No%20Doses"
  }, {
    "title": "Immunize Patient for Hib - No Doses",
    "description": "Provide Hib immunizations – using the \"Hib Vaccine immunization 3p schedule – NO PREVIOUS\" schedule (3 dose scheme)",
    "condition": [ {
      "kind": "applicability",
      "expression": {
        "description": "\"Hib vaccine immunization history\" = \"No-doses and >= 6 weeks old and no known allergies to any component\nof the vaccine and In a country where the peak burden of severe Hib disease occurs in young infants\"",
        "language": "text/cql",
        "expression": "Should vaccinate patient for Hib because no doses"
      }
    } ],
    "definitionCanonical": "Https://placeholder.org/fhir/ActivityDefinition/Immunize%20Patient%20for%20Hib%20-%20No%20Doses"
  }, {
    "title": "Immunize Patient for Hib - 1 Dose",
    "description": "Provide Hib immunizations – using the \"Hib Vaccine immunization 3p schedule – 1 PREVIOUS\" schedule (3 dose scheme)",
    "condition": [ {
      "kind": "applicability",
      "expression": {
        "description": "\"Hib vaccine immunization history\" = \"1-dose given 4 weeks ago and >= 6 weeks old and no known allergies to any component\nof the vaccine and In a country where the peak burden of severe Hib disease occurs in young infants\"",
        "language": "text/cql",
        "expression": "Should vaccinate patient for Hib because 1 dose"
      }
    } ],
    "definitionCanonical": "Https://placeholder.org/fhir/ActivityDefinition/Immunize%20Patient%20for%20Hib%20-%201%20Dose"
  }, {
    "title": "Immunize Patient for Hib - 2 Doses",
    "description": "Provide Hib immunizations – using the \"Hib Vaccine immunization 3p schedule – 2 PREVIOUS\" schedule (3 dose scheme)",
    "condition": [ {
      "kind": "applicability",
      "expression": {
        "description": "\"Hib vaccine immunization history\" = \"2-doses given 4 weeks ago and >= 6 weeks old and no known allergies to any component\nof the vaccine and In a country where the peak burden of severe Hib disease occurs in young infants\"",
        "language": "text/cql",
        "expression": "Should vaccinate patient for Hib because 2 doses"
      }
    } ],
    "definitionCanonical": "Https://placeholder.org/fhir/ActivityDefinition/Immunize%20Patient%20for%20Hib%20-%202%20Doses"
  }, {
    "title": "Immunize Patient for Hib - No Doses",
    "description": "Provide Hib immunizations – using the \"Hib Vaccine immunization 3p + 1 schedule – NO PREVIOUS\" schedule (3 dose scheme)",
    "condition": [ {
      "kind": "applicability",
      "expression": {
        "description": "\"Hib vaccine immunization history\" = \"No-doses and >= 6 weeks old and no known allergies to any component\nof the vaccine and In a setting (e.g. where the greatest disease morbidity and mortality occur later, or where\nrate reductions of disease are not fully sustained after the routine use of Hib vaccine),\"",
        "language": "text/cql",
        "expression": "Should vaccinate patient for Hib because No dose"
      }
    } ],
    "definitionCanonical": "Https://placeholder.org/fhir/ActivityDefinition/Immunize%20Patient%20for%20Hib%20-%20No%20Doses"
  }, {
    "title": "Immunize Patient for Hib - 1 Dose",
    "description": "Provide Hib immunizations – using the \"Hib Vaccine immunization 3p + 1 schedule – 1 PREVIOUS\" schedule (3 dose scheme)",
    "condition": [ {
      "kind": "applicability",
      "expression": {
        "description": "\"Hib vaccine immunization history\" = \"1-dose given 4 weeks ago and >= 6 weeks old and no known allergies to any component\nof the vaccine and In a setting (e.g. where the greatest disease morbidity and mortality occur later, or where\nrate reductions of disease are not fully sustained after the routine use of Hib vaccine),\"",
        "language": "text/cql",
        "expression": "Should vaccinate patient for Hib because 1 dose"
      }
    } ],
    "definitionCanonical": "Https://placeholder.org/fhir/ActivityDefinition/Immunize%20Patient%20for%20Hib%20-%201%20Dose"
  }, {
    "title": "Immunize Patient for Hib - 2 Doses",
    "description": "Provide Hib immunizations – using the \"Hib Vaccine immunization 3p + 1 schedule – 2 PREVIOUS\" schedule (3 dose scheme)",
    "condition": [ {
      "kind": "applicability",
      "expression": {
        "description": "\"Hib vaccine immunization history\" = \"2-doses given 4 weeks ago and >= 6 weeks old and no known allergies to any component\nof the vaccine and In a setting (e.g. where the greatest disease morbidity and mortality occur later, or where\nrate reductions of disease are not fully sustained after the routine use of Hib vaccine),\"",
        "language": "text/cql",
        "expression": "Should vaccinate patient for Hib because 2 doses"
      }
    } ],
    "definitionCanonical": "Https://placeholder.org/fhir/ActivityDefinition/Immunize%20Patient%20for%20Hib%20-%202%20Doses"
  }, {
    "title": "Immunize Patient for Hib - No Doses",
    "description": "Provide Hib immunizations – using the \"Hib Vaccine immunization 2p + 1 schedule – NO PREVIOUS\" schedule (2 dose scheme)",
    "condition": [ {
      "kind": "applicability",
      "expression": {
        "description": "\"Hib vaccine immunization history\" = \"No-doses and >= 6 weeks old and no known allergies to any component\nof the vaccine and In a setting (e.g. where the greatest disease morbidity and mortality occur later, or where\nrate reductions of disease are not fully sustained after the routine use of Hib vaccine),\"",
        "language": "text/cql",
        "expression": "Should vaccinate patient for Hib because No dose"
      }
